#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/Greg/Projects/apps/zyp/zyp-community/workspace/zyp-platform/node_modules/.pnpm/vitest@3.2.4_@types+node@20.17.0_@vitest+ui@4.0.7_happy-dom@20.0.10_jiti@2.6.1_jsdom@27.1.0_t_t4uup5lq5dymnndpky2jbvy7hq/node_modules/vitest/node_modules:/Users/Greg/Projects/apps/zyp/zyp-community/workspace/zyp-platform/node_modules/.pnpm/vitest@3.2.4_@types+node@20.17.0_@vitest+ui@4.0.7_happy-dom@20.0.10_jiti@2.6.1_jsdom@27.1.0_t_t4uup5lq5dymnndpky2jbvy7hq/node_modules:/Users/Greg/Projects/apps/zyp/zyp-community/workspace/zyp-platform/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/Greg/Projects/apps/zyp/zyp-community/workspace/zyp-platform/node_modules/.pnpm/vitest@3.2.4_@types+node@20.17.0_@vitest+ui@4.0.7_happy-dom@20.0.10_jiti@2.6.1_jsdom@27.1.0_t_t4uup5lq5dymnndpky2jbvy7hq/node_modules/vitest/node_modules:/Users/Greg/Projects/apps/zyp/zyp-community/workspace/zyp-platform/node_modules/.pnpm/vitest@3.2.4_@types+node@20.17.0_@vitest+ui@4.0.7_happy-dom@20.0.10_jiti@2.6.1_jsdom@27.1.0_t_t4uup5lq5dymnndpky2jbvy7hq/node_modules:/Users/Greg/Projects/apps/zyp/zyp-community/workspace/zyp-platform/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
